{
  "expo": {
    "web": {
      "favicon": "./assets/images/pwa-icon.png",
      "bundler": "metro",
      "display": "standalone",
      "startUrl": "/?utm_source=web_app_manifest",
      "scope": "/",
      "orientation": "portrait",
      "themeColor": "#000000",
      "backgroundColor": "#000000",
      "splash": {
        "image": "./assets/images/pwa-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "pwa": true
    },
    "name": "Quilox",
    "slug": "quiloxluxury",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "quiloxluxury",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quilox.luxury",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Quilox needs access to your photo library to update your profile picture and share content.",
        "NSCameraUsageDescription": "Quilox needs access to your camera to take profile pictures.",
        "NSLocationWhenInUseUsageDescription": "Quilox uses your location to show nearby venues and enhance your booking experience.",
        "NSUserTrackingUsageDescription": "This allows us to provide personalized content and analyze app performance to improve your experience.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.quilox.luxury",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d5e59b5f-c6fc-40aa-b3c9-80dd9e5e385e"
      }
    },
    "owner": "clubquiloxapp",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d5e59b5f-c6fc-40aa-b3c9-80dd9e5e385e"
    }
  }
}
